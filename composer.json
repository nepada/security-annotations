{
    "name": "nepada/security-annotations",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Security annotations for Nette presenters and components.",
    "keywords": ["security", "annotations", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/security-annotations/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/utils": "^3.0@dev",
        "nette/component-model": "^3.0@dev",
        "nette/application": "^3.0@dev",
        "nette/security": "^3.0@dev",
        "nette/http": "^3.0.1@dev",
        "doctrine/annotations": "^1.8.0@dev"
    },
    "require-dev": {
        "nette/tester": "^2.3.2",
        "mockery/mockery": "^1.4.0",
        "phpstan/phpstan": "^0.12.27",
        "phpstan/phpstan-strict-rules": "^0.12.2",
        "pepakriz/phpstan-exception-rules": "^0.11.1",
        "phpstan/phpstan-nette": "^0.12.6",
        "nepada/phpstan-nette-tester": "^0.3.0",
        "spaze/phpstan-disallowed-calls": "^0.12.1",
        "phpstan/phpstan-mockery": "^0.12.5",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "nepada/coding-standard": "^7.1.1",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.1@dev",
        "nettrine/annotations": "^0.6.0@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\SecurityAnnotations\\": "src/SecurityAnnotations/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    }
}
